            <script type="text/template" ne-foreach="localNews in localNewsList">
                    <%if(__i%6==0){%>
                         <%if(__i==0){%>
                            <ul class="cm_ul_round">
                        <%}else{%>
                            </ul>
                            <ul class="cm_ul_round">
                        <%}%>
                        <li class="cm_fb">
                           <%if(localNews.url != '' && localNews.title != ''){%>
